{
    "name": "§6[⏺] §rMist Form",
    "description": "You bend your energy at will, turning into pure white mist.",
    "badges": [
        {
            "type": "origins:keybind",
            "text": "Secondary ability, toggle with %s.",
            "sprite": "origins:textures/gui/badge/toggle.png"
        },
        {
            "type": "origins:tooltip",
            "text": "When in mist form:\n-Cannot fly with an elytra\n-Cannot use other active powers (relocation and energy beam)\n-Cannot attack mobs\n-Cannot break/place blocks\n-Cannot interact with entities\n-Disables creature detection\n-Invulnerable to all attacks\n-You can phase through blocks\n-Sprint to fly",
            "sprite": "origins:textures/gui/badge/info.png"
        }
    ],
    "type": "origins:multiple",
    "toggle": {
        "condition": {
            "inverted": true,
            "type": "origins:power_active",
            "power": "*:fly_toggle"
        },
        "type": "origins:toggle",
        "active_by_default": false,
        "key": {
            "key": "key.origins.secondary_active"
        },
        "retain_state": true
    },
    "invisibility": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:invisibility",
        "render_armor": false,
        "render_outline": false
    },
    "particles": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:action_over_time",
        "interval": 2,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:end_rod ~ ~1 ~ 1.2 1.2 1.2 0 2 force"
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:end_rod ~ ~0.5 ~ 1 1 1 .01 2 force"
                }
            ]
        }
    },
    "phasing": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:phasing",
        "block_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:in_tag",
                    "tag": "origins:unphasable"
                },
                {
                    "inverted": true,
                    "type": "origins:in_tag",
                    "tag": "origins:unphasable"
                }
            ]
        },
        "blacklist": true,
        "render_type": "none",
        "view_distance": 2147483647,
        "phase_down_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:sneaking"
                }
            ]
        }
    },
    "up": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "*:*_toggle"
                },
                {
                    "type": "origins:in_block",
                    "block_condition": {
                        "type": "origins:movement_blocking"
                    }
                }
            ]
        },
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:apply_effect",
            "effect": {
                "effect": "minecraft:levitation",
                "duration": 2,
                "amplifier": 10,
                "show_particles": false,
                "show_icon": false
            }
        },
        "cooldown": 0,
        "key": {
            "key": "key.jump",
            "continuous": true
        }
    },
    "deplete": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:action_over_time",
        "interval": 30,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "resource": "*:charge_energy",
                "comparison": ">=",
                "compare_to": 6
            },
            "if_action": {
                "type": "origins:change_resource",
                "resource": "*:charge_energy",
                "change": -5,
                "operation": "add"
            },
            "else_action": {
                "type": "origins:execute_command",
                "command": "damage @s 1 minecraft:magic"
            }
        }
    },
    "resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 1,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_01.png",
            "bar_index": 9,
            "condition": {
                "type": "origins:power_active",
                "power": "*:*_toggle"
            }
        }
    },
    "swim": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "inverted": true,
                    "type": "origins:submerged_in",
                    "fluid": "water"
                },
                {
                    "type": "origins:power_active",
                    "power": "*:*_toggle"
                }
            ]
        },
        "type": "origins:swimming"
    },
    "soundstop": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "stopsound @a * minecraft:ambient.underwater.loop"
                },
                {
                    "type": "origins:execute_command",
                    "command": "stopsound @a * minecraft:ambient.underwater.loop.additions"
                },
                {
                    "type": "origins:execute_command",
                    "command": "stopsound @a * minecraft:ambient.underwater.loop.additions.rare"
                },
                {
                    "type": "origins:execute_command",
                    "command": "stopsound @a * minecraft:ambient.underwater.loop.additions.ultra_rare"
                },
                {
                    "type": "origins:execute_command",
                    "command": "stopsound @a * minecraft:entity.generic.swim"
                },
                {
                    "type": "origins:execute_command",
                    "command": "stopsound @a * minecraft:entity.player.swim"
                }
            ]
        }
    },
    "item": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:prevent_item_use"
    },
    "block": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:prevent_block_use"
    },
    "select": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:prevent_block_selection"
    },
    "entity": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:prevent_entity_use"
    },
    "hit": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:modify_damage_dealt",
        "modifier": {
            "operation": "multiply_total",
            "value": -1
        }
    },
    "elytra": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:prevent_elytra_flight"
    },
    "actions_on_toggle": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:block.respawn_anchor.charge",
                    "volume": 0.3,
                    "pitch": 2
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:item.lodestone_compass.lock",
                    "volume": 1,
                    "pitch": 2
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:block.beacon.activate",
                    "volume": 0.15,
                    "pitch": 2
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:entity.glow_squid.squirt",
                    "volume": 1,
                    "pitch": 2
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:item.armor.equip_netherite",
                    "volume": 1,
                    "pitch": 1.25
                },
                {
                    "type": "origins:spawn_particles",
                    "particle": "minecraft:end_rod",
                    "count": 100,
                    "speed": 1,
                    "force": true,
                    "spread": {
                        "x": 0.1,
                        "y": 0.1,
                        "z": 0.1
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "scale set pehkui:visibility -1 @s"
                },
                {
                    "type": "origins:area_of_effect",
                    "radius": 20,
                    "bientity_action": {
                        "type": "origins:target_action",
                        "action": {
                            "type": "origins:execute_command",
                            "command": "data modify entity @s hunted_recently set value 0",
                            "permission_level": 4
                        }
                    }
                }
            ]
        },
        "falling_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:block.respawn_anchor.deplete",
                    "volume": 0.6,
                    "pitch": 2
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:item.lodestone_compass.lock",
                    "volume": 1,
                    "pitch": 1.5
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:entity.glow_squid.squirt",
                    "volume": 0.7,
                    "pitch": 2
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:item.armor.equip_netherite",
                    "volume": 1,
                    "pitch": 1
                },
                {
                    "type": "origins:spawn_particles",
                    "particle": "minecraft:end_rod",
                    "count": 100,
                    "speed": 1,
                    "force": true,
                    "spread": {
                        "x": 0.1,
                        "y": 0.1,
                        "z": 0.1
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "scale reset pehkui:visibility @s"
                }
            ]
        }
    },
    "overlay": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "type": "origins:overlay",
        "texture": "minecraft:textures/misc/nausea.png",
        "strength": 0.2,
        "red": 1,
        "green": 1,
        "blue": 1,
        "draw_mode": "texture",
        "draw_phase": "below_hud",
        "hide_with_hud": false,
        "visible_in_third_person": true
    }
}